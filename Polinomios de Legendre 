        "# --------- (a) Graficar Pn(x) para n = 0 a 5 ---------\n",
        "x_vals = np.linspace(-1, 1, 400)\n",
        "plt.figure(figsize=(10, 6))\n",
        "for n in range(6):\n",
        "    Pn_func = legendre(n)\n",
        "    plt.plot(x_vals, Pn_func(x_vals), label=f'$P_{n}(x)$')\n",
        "plt.title('Polinomios de Legendre $P_n(x)$ para $n=0$ a $5$')\n",
        "plt.xlabel('$x$')\n",
        "plt.ylabel('$P_n(x)$')\n",
        "plt.grid()\n",
        "plt.legend()\n",
        "plt.show()"

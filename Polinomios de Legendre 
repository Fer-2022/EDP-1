        "# --------- (e) Verificar f√≥rmula de Rodrigues ---------\n",
        "print(\"\\n(e) Verificaci√≥n de la f√≥rmula de Rodrigues:\")\n",
        "for n in range(6):\n",
        "    rodrigues = (1 / (2**n * factorial(n))) * sp.diff((x**2 - 1)**n, x, n)\n",
        "    Pn_leg = sp.legendre(n, x)\n",
        "    cumple = sp.simplify(rodrigues - Pn_leg) == 0\n",
        "    print(f\"n = {n}: ¬øRodrigues = P_{n}(x)? {cumple}\")"
